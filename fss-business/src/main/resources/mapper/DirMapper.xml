<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zx.fss.mapper.DirMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zx.fss.business.Dir">
        <id column="dir_id" property="dirId" />
        <result column="name" property="name" />
        <result column="parent_id" property="parentId" />
        <result column="user_id" property="userId" />
        <result column="path" property="path" />
        <result column="creation_date" property="creationDate" />
        <result column="created_by" property="createdBy" />
        <result column="last_update_date" property="lastUpdateDate" />
        <result column="last_updated_by" property="lastUpdatedBy" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        dir_id, name, parent_id, user_id, path, creation_date, created_by, last_update_date, last_updated_by, is_deleted
    </sql>

</mapper>

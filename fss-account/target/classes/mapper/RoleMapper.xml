<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zx.fss.mapper.RoleMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zx.fss.account.Role">
        <id column="role_id" property="roleId" />
        <result column="name" property="name" />
        <result column="ename" property="ename" />
        <result column="description" property="description" />
        <result column="enabled_flag" property="enabledFlag" />
        <result column="creation_date" property="creationDate" />
        <result column="created_by" property="createdBy" />
        <result column="last_update_date" property="lastUpdateDate" />
        <result column="last_updated_by" property="lastUpdatedBy" />
        <result column="is_deleted" property="isDeleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        role_id, name, ename, description, enabled_flag, creation_date, created_by, last_update_date, last_updated_by, is_deleted
    </sql>

</mapper>
